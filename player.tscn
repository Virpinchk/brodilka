func _physics_process(delta):
	get_input()
	velocity = move_and_slide(velocity)
	if Input.is_action_just_pressed(\"shut\") and $kuldaun.is_stopped():
		
		var object = $shut.get_collider() as Node2D
		if object and object.is_in_group(\"enemy\"):
			object.dead()
		$kuldaun.start()
func _process(delta):
	look_at(get_global_mouse_position())


func _ready():
	$\"/root/Global\".player = self

